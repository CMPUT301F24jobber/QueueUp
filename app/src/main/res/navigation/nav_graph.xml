<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/nav_host_fragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:navGraph="@navigation/nav_graph" />

    <fragment
        android:id="@+id/organizerWaitingListFragment"
        android:name="com.example.queueup.views.organizer.OrganizerWaitingListFragment">
        <action
            android:id="@+id/action_organizerWaitingListFragment_to_organizerMap"
            app:destination="@id/organizerMap" />
    </fragment>

    <fragment
        android:id="@+id/organizerMap"
        android:name="com.example.queueup.views.organizer.OrganizerMap"
        android:label="OrganizerMap">
        <argument
            android:name="selected_user"
            app:argType="com.example.queueup.models.User" />
    </fragment>

</androidx.constraintlayout.widget.ConstraintLayout>